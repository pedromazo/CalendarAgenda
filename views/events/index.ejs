<div class="row">
    <div class="col-8">
            <h2>Meus Eventos:</h2>
        <% for (let event of eventos) {%>
            <% let data =new Date(Date(event.initDate)) %> 
        <h3><%=data.getDate()%> - <%= data.getMonth()+1 %> - <%= data.getFullYear() %>  </h3>
        <ul>
            <li><a href="events/<%=event._id%>"><%= event.eventName %></a></li>
        
        </ul>
        <% } %> 
    </div>
